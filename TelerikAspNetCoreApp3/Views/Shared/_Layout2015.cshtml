    <style>
        .closedTab {
            top: 7px;
            position: relative;
            left: 5px;
            border-radius: 50%;
        }

            .closedTab:hover {
                background-color: red;
            }

        html {
            background-color: #e2e2e2;
            margin: 0;
            padding: 0;
        }


        .naser {
            background-color: #fff;
            color: #333;
            background: url(../../../../Images/background.png) repeat-x scroll 50% 0 #000000;
            color: #666666;
        }

        ul#menu {
            font-size: 1.3em;
            font-weight: 600;
            margin: 0 0 5px;
            padding: 0;
            text-align: right;
        }

            ul#menu li {
                display: inline;
                list-style: none;
                padding-left: 15px;
            }

                ul#menu li a {
                    background: none;
                    color: #999;
                    text-decoration: none;
                }

                    ul#menu li a:hover {
                        color: #333;
                        text-decoration: none;
                    }
    </style>
    <script type="text/javascript">
        $('html').height(window.innerHeight);
        $(window).resize(function () {
            if (window.innerWidth < 600) {
                $('#myMenu').hide();
            }
            else {
                $('#myMenu').show();
            }
        });

    </script>
