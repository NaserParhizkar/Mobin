    <script id="emailTemplate" type="text/x-kendo-template">
        <div class="new-mail">
            <img src="../../../content/web/notification/envelope.png" />
            <h3>#= title #</h3>
            <p>#= message #</p>
        </div>
    </script>


    <script id="errorTemplate" type="text/x-kendo-template">
        <div class="k-widget k-notification k-notification-error k-popup k-group k-reset k-state-border-up"
             style="width: 200px;padding:5px 20px 5px 20px;color:white;
                    background:none repeat scroll 0 0 rgba(100%, 0%, 0%, 0.7);">
            <div>
                <img src='@Url.Content("~/Content/notification/error-icon.png")' style="float: left;" />
                <h3 style="color:white">#= title #</h3>
                <p style="font-size:16px;">#= message #</p>
            </div>
        </div>
    </script>


    <script id="successTemplate" type="text/x-kendo-template">
        <div class="upload-success"
             style=" width:250px;padding:5px 20px 5px 20px;color:white;">
            <div>
